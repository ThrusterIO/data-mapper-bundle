{
    "name": "thruster/data-mapper-bundle",
    "type": "symfony-bundle",
    "description": "Thruster Mapper Bundle",
    "keywords": ["symfony", "bundle", "datamapper", "data-mapper", "thruster"],
    "homepage": "https://thruster.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": ">=5.4",
        "thruster/data-mapper": ">=1.0,<2.0",
        "symfony/http-kernel": ">= 2.5",
        "symfony/dependency-injection": ">= 2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": { "Thruster\\Bundle\\DataMapperBundle\\": "src" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit -c phpunit.xml.ci"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
